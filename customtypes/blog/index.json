{
  "id": "blog",
  "label": "Blog",
  "format": "custom",
  "repeatable": true,
  "status": true,
  "json": {
    "Blog": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "date_published": {
        "type": "Date",
        "config": {
          "label": "Date Published",
          "placeholder": ""
        }
      },
      "content": {
        "type": "StructuredText",
        "config": {
          "label": "Content",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item"
        }
      }
    }
  }
}
